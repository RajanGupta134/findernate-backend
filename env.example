# Database Configuration
MONGODB_URI=your_mongodb_connection_string_here

# Server Configuration
PORT=8000
NODE_ENV=development

# JWT Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRE=7d

# Bunny.net Configuration (for file uploads)
BUNNY_STORAGE_ZONE_NAME=your_bunny_storage_zone_name
BUNNY_ACCESS_KEY=your_bunny_access_key
BUNNY_STORAGE_API_URL=https://storage.bunnycdn.com/your_zone_name
BUNNY_CDN_URL=https://your_zone_name.b-cdn.net


# Email Configuration (for nodemailer)
EMAIL_HOST=your_email_host
EMAIL_PORT=587
EMAIL_USER=your_email_username
EMAIL_PASS=your_email_password

# Web Push Notifications
VAPID_PUBLIC_KEY=your_vapid_public_key
VAPID_PRIVATE_KEY=your_vapid_private_key
VAPID_EMAIL=your_email_for_vapid

# CORS Origins (add your frontend URLs)
FRONTEND_URL=http://localhost:3000

# 100ms Configuration
HMS_ACCESS_KEY=your_100ms_access_key_here
HMS_SECRET=your_100ms_secret_here
HMS_VOICE_TEMPLATE_ID=your_voice_template_id
HMS_VIDEO_TEMPLATE_ID=your_video_template_id